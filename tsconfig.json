{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020", "DOM"],
    "jsx": "react",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  "outDir": "./dist",
  "rootDir": ".",
    "types": ["jest", "node"]
  },
  "include": ["src", "test"],
  "exclude": ["node_modules", "dist"]
}
